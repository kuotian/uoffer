/* pages/index/search/search.wxss */
page{
  background: #eeeeee;
}
image{
  width: 100%;
  height: 100%;

}
/*  */
.history {
	padding: 0 20px;
    padding-top: 40rpx;
}

.no-result-history {
    width: 600rpx;
}

.history-title {
    font-size: 18px;
    color: #797A7B;
}

.del-history {
    color: #797A7B;
    font-size: 12px;
}

.del-icon {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.records {
    margin-top: 40rpx;
}

.records-item {
    min-width: 98rpx;
    display: inline-block;
    background: #F3F3F3;
    color: #4D5D64;
    font-size: 20rpx;
    text-align: center;
    border-radius: 20rpx;
    padding: 10rpx 20rpx;
    margin-bottom: 30rpx;
    box-sizing: content-box;
    margin-right: 25rpx;
}
.title {
    font-size: 18px;
    color: #24292F;
}
/*  */
.search{
  border-radius: 45rpx;
  margin-left: 30rpx;
  width: 600rpx;
  height: 70rpx;
  background-color: #eeeeee;
}
.search input{
  width: 400rpx;
  font-size: 28rpx;
  color: #999999;
  margin-left: 25rpx;
  float: left;
  height: 70rpx;
}
.page_row{
  display:flex;
  align-items:center;
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.searchcion {
  margin-left: 24rpx;
  margin-top: 11rpx;
  float: left;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}
.onsearch{
  font-size: 32rpx;
  color: #999999;
  line-height: 80rpx;
  margin-left: 30rpx;
}

.navcontent{
  width: 100%;
  border-top:1rpx solid #eeeeee;
  display: flex;
  background: #fff;
  position: relative;
}
.nav_one{
  flex: 1;
  line-height: 80rpx;
  text-align: center;
  color: #333333;
}
.active{
  color: #2AA2DF;
}
.nav_oneText{
  line-height: 80rpx;
  font-size: 28rpx;
  display: inline-block;
}
.nav_one image{
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  margin-left:15rpx;
}
.table{
  width: 100%;
  padding-bottom:20rpx;
  background: #fff;
  margin-top: 20rpx;
}
.table_head{
  width: 95%;
  padding: 20rpx 0rpx;
  margin: 0 auto;
  position: relative;
}
.table_name{
  width: 100%;
  float: left;
  font-size: 30rpx;
  color: #333;
}
.table_name text{
  float: right;
  font-size: 26rpx;
  color: #999;
}
.table_type{
  width: 100%;
  font-size: 28rpx;
  line-height: 60rpx;
}
.type_left{
  color: #2AA2DF;
}
.type_right{
  color: #666666;
  padding: 0rpx 20rpx;
  border-left:2rpx solid #ccc; 
  margin-left: 20rpx;
}
.table_address{
  font-size: 22rpx;
  color: #999999;
}
.table_but{
  position: absolute;
  font-size: 26rpx;
  color: #fff;
  padding: 10rpx 20rpx;
  background: #2AA2DF;
  bottom: 20rpx;
  right: 0rpx;
  border-radius:10rpx; 
}
.table_buzhu{
  width: 95%;
  padding-top:6rpx;
  margin: 0 auto;
}
.table_text{
  font-size: 22rpx;
  color: #109bc2;
  padding: 5rpx 20rpx;
  background: #d7f6ff;
  border-radius:10rpx; 
  display:inline;
  margin: 10rpx;
}

.bgcolor{
  background: rgba(0, 0, 0, 0.3);
  width: 100%;
  position: absolute;
  bottom:0;
  left: 0; 
}
.bgcolor1{
  width: 100%;
  height: 90rpx;
  background: #ffffff;
  border-top:1rpx solid #f5f5f5;
}
.bgcolor_text{
  line-height: 90rpx;
}
.select{
  width: 6rpx;
  height: 90rpx;
  background: #2AA2DF;
  float: left;
}
.bgcolor_text text{
  font-size: 28rpx;
  color: #333333;
  margin-left: 30rpx;
}