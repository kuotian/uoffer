/* pages/user/jianli/jianli.wxss */
page{
  background: #f5f5f5;
}
.one{
  width: 100%;
  background: #ffffff;
  border-top: 1rpx solid #f5f5f5;
  margin-bottom: 20rpx;
  position: relative;
}
.one_title{
  padding:0 20rpx; 
  font-size: 30rpx;
  color: #333333;
  line-height: 70rpx;
  margin-top:20rpx;
}
.default{
	width: 10px;
	height: 10px;
	border-radius: 50%;
	background: #2AA2DF;
}
.one_title text{
  margin-left: 50rpx;
}
.one_time{
  padding:0 20rpx;
   padding-bottom: 10px;
  font-size: 24rpx;
  color: #999999;
  line-height:50rpx;
}
.one_time text{
  float: right;
}
.one_wanzheng{
  height: 50rpx;
  margin-left:20rpx; 
  font-size: 24rpx;
  color: #999999;
  padding-bottom:20rpx;
}
progress{
  width: 220rpx;
  float: left;
}
.one_wanzheng text{
  float: right;
  margin-right: 20rpx;
}
.button{
  width: 100%;
  display: flex;
  padding: 50rpx 0;
}
.button1{
  flex: 1;
  
}
.button1_one{
  width: 80%;
  height: 100rpx;
  line-height: 100rpx;
  background: #2AA2DF;
  font-size: 30rpx;
  color: #ffffff;
  text-align: center;
  border-radius:10rpx; 
  margin: auto;
}
.button2{
  flex: 1;
}
.button2_one{
  width: 80%;
  height: 100rpx;
  line-height: 100rpx;
  background: #ffffff;
  font-size: 30rpx;
  color: #333333;
  text-align: center;
  border: 1rpx solid #cccccc;
  border-radius:10rpx; 
  margin: auto;
}

.two{
  width: 100%;
  height: 160rpx;
  z-index:10;
  position: relative;
  margin-top: -15rpx;
}
.two_bg{
  width: 100%;
  height: 100%;
}
.two1{
  width: 100%;
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
}
.three{
  flex: 1;
  width: 25%;
  text-align: center;
  margin-top: 25rpx;
}
.three_img{
  width: 70rpx;
  height: 70rpx;
}
.three_text{
  font-size: 28rpx;
  color: #ffffff;
}