.table{
  width: 100%;
  padding-bottom:20rpx;
  background: #fff;
}
.table_head{
  width: 95%;
  border-bottom:1rpx solid #eeeeee;
  padding: 20rpx 0rpx;
  margin: 0 auto;
  position: relative;
}
.table_name{
  width: 100%;
  float: left;
  font-size: 30rpx;
  color: #333;
}
.table_name text{
  float: right;
  font-size: 26rpx;
  color: #999;
}
.table_type{
  width: 100%;
  font-size: 28rpx;
  line-height: 60rpx;
}
.type_left{
  color: #2AA2DF;
}
.type_right{
  color: #666666;
  padding: 0rpx 20rpx;
  border-left:2rpx solid #ccc; 
  margin-left: 20rpx;
}
.table_address{
  font-size: 22rpx;
  color: #999999;
}