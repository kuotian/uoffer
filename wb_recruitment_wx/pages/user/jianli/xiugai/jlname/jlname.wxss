page{
  background: #f5f5f5;
}
.lest{
  width: 100%;
  height: 100rpx;
  background: #ffffff;
  border-top: 1rpx solid #f5f5f5;
  position: relative;
}
.lest_input{
  width: 90%;
  height: 100rpx;
  margin: 0 auto;
  font-size: 30rpx;
}
.lest_img{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 24rpx;
  right: 20rpx;
}
.determine{
  width: 100%;
  height: 98rpx;
  background-color: #fe853c;
  font-size: 40rpx;
  color: #fff;
  text-align: center;
  line-height: 98rpx;
  letter-spacing:5px;
  position: fixed;
  z-index: 1000;
  bottom: 0;
  right: 0;
  border-radius: 0;
  border: 0;
}
.determine::after {
    border-radius: 0;
    border: 0;
}