/**index.wxss**/
.todo-header {
  background: #F5F8FF;
  display: block;
  font-size: 28rpx;
  padding: 12rpx 30rpx;
  color: #9198AB;
}

.todo-list {
  background: #fff;
}

.todo-list__item {

}

/* todo-add */
.todo-add {
  position: fixed;
  left: 50%;
  bottom: 60rpx;
  width: 90rpx;
  height: 90rpx;
  margin-left: -45rpx;
  border-radius: 50%;
  font-size: 60rpx;
  line-height: 90rpx;
  text-align: center;
  background-color: #fff;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.1);
  color: #444;
}

.search{
  border-radius: 45rpx;
  margin-left: 30rpx;
  width: 600rpx;
  height: 70rpx;
  background-color: #eeeeee;
}
.search picker{
  width: 400rpx;
  font-size: 28rpx;
  color: #999999;
  margin-left: 25rpx;
  float: left;
  height: 70rpx;
  line-height: 70rpx;
}
.page_row{
  display:flex;
  align-items:center;
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.searchcion {
  margin-left: 24rpx;
  margin-top: 11rpx;
  float: left;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}
.onsearch{
  font-size: 32rpx;
  color: #999999;
  line-height: 80rpx;
  margin-left: 30rpx;
}